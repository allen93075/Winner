        <widget class="QLabel" name="label_7">
         <property name="text">
          <string>Inputs: LookBack(10), Rang(0.4), When(4);Vars: HighValue(0), LowValue(1000000),Tr(0);Inputs: InitialCapital(10000000), Delta(20000);//Fixed RatioInputs: InCloudOpenPL(false), Maxsize(10);Vars: contractAmt(0), ExitAmount(0), Equity(0), MP(0), pTS(0);LowValue = Lowest(L, LookBack);HighValue = Highest(H, LookBack);Tr = (HighValue - LowValue)*Range;//Fixed Ratioif InCloudOpenPL then Equity = Round((InitialCapital + Netprofit + OpenPositionProfit), 0)	else Equity = Round((InitialCapital + NetProfit), 0);	//Position Sizing - Fixed Ratioif Delta &gt; 0 and Equity/Delta &gt; 0 then	contractAmt = 0.5*(1 + SquareRoot(1 + 8*Equity/Delta));	contractAmt = MaxList(1, IntPortion(ContractAmt));	if contractAmt &lt; 1 then ContractAmt = 1;if contractAmt &gt; MaxSize then ContractAmt = Maxsize;pTS = contractAmt;//RangeBreakif Marketposition = 0 then	Buy(&quot;buy&quot;) pTS shares next bar closed(1) + Tr stop;if ExitAmount = ContractAmt then	sellshort(&quot;sell&quot;)pTS shares next bar closed(1) - Tr stop;If Close &lt; LowValue[1] Thensell(&quot;EXIT_buy&quot;) pTS shares next bar market;If Close &gt; HighValue[1] Thenbuytocover(&quot;EXIT_sell&quot;) pTS shares next bar market;If barssinceentry = When and openpositionprofit &lt; 0 then beginsell pTS shares next bar market;buytocover pTS shares next bar market;end;</string>
