      <layout class="QGridLayout" name="gridLayout_3">
       <item row="1" column="0">
        <widget class="QLabel" name="label_5">
         <property name="text">
          <string>Inputs: LookBack(10), Rang(0.4), When(4);Vars: HighValue(0), LowValue(1000000),Tr(0);//-----Kelly-----(1)input:InitialCapital(1000000), MaxSize(10);var:pTs(1),b(0),p(0),q(0),f(0),pCapitalperContract(200000),ExitAmount(0),MP(0);//K%=W-(1-W)/Rif Grossprofit&lt;&gt;0 and Totaltrades&lt;&gt;0 and grossloss&lt;&gt;0 and   Numlostrades &lt;&gt;0 then b = (Grossprofit/ numwintrades)/(grossloss/ Numlostrades);if Numwintrades&lt;&gt;0 and Totaltrades&lt;&gt;0 then     p = Numwintrades/ Totaltrades;     q = 1-p;if b&lt;&gt;0 and p&lt;&gt;0 then     f = p-(q/b);if f&lt;&gt;0 and f*(InitialCapital+netprofit)&gt;0 then        pTS =IntPortion((f*(InitialCapital+netprofit))/pCapitalperContract); if pTS &lt; 1 then pTS = 1 ;  if pTS &gt; MaxSize then pTS = MaxSize ;//RangeBreakLowValue = Lowest(L, LookBack);HighValue = Highest(H, LookBack);Tr = (HighValue - LowValue)*Range;Buy(&quot;buy&quot;) pTS shares next bar closed(1) + Tr stop;sellshort(&quot;sell&quot;) pTS shares next bar closed(1) - Tr stop;If Close &lt; LowValue[1] Thensell(&quot;EXIT_buy&quot;) pTS shares next bar market;If Close &gt; HighValue[1] Thenbuytocover(&quot;EXIT_sell&quot;) pTS shares next bar market;If barssinceentry = When and openpositionprofit &lt; 0 then beginsell pTS shares next bar market;buytocover pTS shares next bar market;end;</string>
